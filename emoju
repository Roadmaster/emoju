#!/usr/bin/env python3
import sys


# thinking_face
# face_with_medical_mask
# clipboard
# face_vomiting
# hammer_and_wrench
substitutions = {
    "ğŸ¤”": "status",
    "ğŸ": "deploy",
    "ğŸ› ": "config",
    "ğŸ˜·": "debug-logs",
    "ğŸ“‹": "tabular",
    "ğŸ¤®": "yaml",
}

args = ['juju']

for word in sys.argv[1:]:
    for emoji, meaning in substitutions.items():
        word = word.replace(emoji, meaning)
    args.append(word)

